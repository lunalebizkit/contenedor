<table class="table table-bordered">
    <thead class="thead-dark">
        <h3> Containers </h3>
        <tr>
            <th scope="col">Numero</th>
            <th scope="col">Marca</th>
            <th scope="col">Capacidad</th>
            <th scope="col">Tara</th>
            <th scope="col">Estado</th>
        </tr>
    </thead>
    <tbody>
        {{#each contenedores}}
        <tr>
                <td>{{NroContenedor}}</td>
                <td>{{marca}}</td>
                <td>{{IdCapacidad}}</td>
                <td>{{Tara}}</td>
                <td>{{estado}}</td>
                
        {{/each}}
    </tbody>
    </form>
    </tr>
    </tbody>
</table>
