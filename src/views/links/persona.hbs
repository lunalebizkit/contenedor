<div class="container p-5">
  <div class="row">
    <div class="col-md-5 mx-auto">
      <div class="card bg-secondary text-white">
        <div class="card-body"> DATOS PERSONALES
             <p class="m-2">Usuario:  {{usuario.usuario}}</p>
             <p class="m-2">Nombre:   {{usuario.nombre}}</p>
             <p class="m-2">Direcci√≥n:  {{usuario.direccion}}</p>
             <p class="m-2">CUIT-CUIL-DNI:  {{usuario.cuit}}</p>
            {{!-- <p>{{timeago created_at}}</p> --}}
            {{!-- <a href="/links/delete/{{id}}" class="btn btn-danger">Borrar link</a> --}}
            <a href="/links/editar/{{usuarios.id}}" class="btn btn-success">Editar</a>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="container p-5">
  <div class="row">
    <div class="col-md-3 mx-auto">
      <div class="card bg-secondary text-white">
        <div class="card-body"> TELEFONO CONTACTO
          {{#each telefono}}
             <p class="m-2">Telefono:  {{telefono}}</p>
