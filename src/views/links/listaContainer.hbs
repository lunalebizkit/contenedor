<div class="container mt-3 mb-5">
    <table class="table table-hover mt-3 mb-5">
        <thead class="table-dark text-center">
            <h3 style="text-align: center;"> <u> Lista de Containers </u></h3>
            <tr>
                <th scope="col">Numero</th>
                <th scope="col">Marca</th>
                <th scope="col">Capacidad (Pies)</th>
                <th scope="col">Tara</th>
                <th scope="col">Estado</th>
            </tr>
        </thead>
        {{#each contenedores}}
        <tbody class="table-warning text-center">            
            <tr>
                <td>{{NroContenedor}}</td>
                <td>{{marca}}</td>
                <td>{{IdCapacidad}}</td>
                <td>{{Tara}}</td>
                <td>{{estado}}</td>
            </tr>
         {{/each}}               
        </tbody>
    </table>
</div>